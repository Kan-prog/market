<header class="navbar navbar-fixed-top">
  <div class="container-fluid">
    <%= link_to "mk mart", root_path, id: "logo" %>
    <nav>
        <% if logged_in? %>
            <ul class="g-nav">
              <li class="mypage"><%= link_to "マイページ", user_path(current_user) %></li>
              <li class="search"><%= link_to "検索", '#' %></li>
              <li class="plus"><%= link_to "出品", "#" %></li>
              <li class="bell"><%= link_to "お知らせ", '#' %></li>
              <li class="logout">
                <%= link_to "ログアウト", logout_path, method: :delete %>
              </li>
            </ul>
        <% else %>
          <ul class="g-nav">
            <li class="search"><%= link_to "検索", "#" %></li>
            <li class="login"><%= link_to "ログイン",login_path %></li>
            <li class="signup"><%= link_to "会員登録", signup_path %></li>
          </ul>  
        <% end %>
    </nav>
  </div>
</header>
